month = {jan},
number = {D1},
pages = {D130--D135},
pmid = {22121212},
title = {{NCBI Reference Sequences (RefSeq): current status, new features and genome annotation policy}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22121212 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3245008 https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gkr1079},
volume = {40},
