journal = {Nucleic Acids Research},
month = {jan},
number = {suppl{\_}1},
pages = {D901--D906},
pmid = {18048412},
publisher = {Oxford University Press},
title = {{DrugBank: a knowledgebase for drugs, drug actions and drug targets}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18048412 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2238889 https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gkm958},
