publisher = {Clinical Chemistry},
title = {{Clinical Trials in Precision Oncology}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26607725 http://clinchem.aaccjnls.org/content/clinchem/62/3/442.full.pdf http://www.clinchem.org/cgi/doi/10.1373/clinchem.2015.247437},
volume = {62},
